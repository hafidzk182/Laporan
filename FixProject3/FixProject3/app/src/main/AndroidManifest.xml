<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hafidz.fixproject3"
    android:usesCleartextTraffic="true">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FixProject3"
        tools:targetApi="31">
        <activity android:name=".ui.gallery.HistoryLkaa"
            android:label="HistoryLaka"
            android:exported="true"/>
        <activity android:name=".ui.gallery.TambahLAKA"
            android:label="Tambah Laka"
            android:exported="true"/>
        <activity
            android:name=".MenutamaActivity"
            android:exported="false"
            android:label="@string/title_activity_menutama"
            android:theme="@style/Theme.FixProject3.NoActionBar" />
        <activity
            android:name=".uilogin.RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".uilogin.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"/>
    </application>

</manifest>